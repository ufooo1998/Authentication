@model List<Authentication.Areas.Identity.Data.AuthenticationUser>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a class="btn btn-success" asp-action="Create">Create</a>
</p>
<div>
    <table class="table table-condensed">
        <thead>
            <tr>
                <th><input type="checkbox" /></th>
                <th>
                    Username
                </th>
                <th>
                    Email
                </th>
                <th>
                    Role
                </th>
                <th>
                    Option
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td><input type="checkbox" /></td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    
                </td>
                <td>
                    <a class="btn btn-primary" asp-action="Edit" asp-route-id="">Edit</a>
                    <a class="btn btn-danger" asp-action="Delete" asp-route-id="">Delete</a>
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>